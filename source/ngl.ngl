// self descriptor (ngl entity describe its concepts)
ngl ngl
{
    environment

    storage
    data
    entity
    concept
    shape
}

ngl data
{
    ngl:shape ngl:shape:ngl
}

ngl:environment computer
{
    ngl:entity os

    ngl:storage ram, hdd

    ngl:data
    {
        element = ngl:byte (8bits)
    }
}